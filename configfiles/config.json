{
  "logRequests": true,
  "listenInfoList": [
    {
      "http3Info": {
        "enabled": true,
        "certFile": "cert.pem",
        "keyFile": "key.pem",
        "overrideAltSvcPortEnabled": false,
        "overrideAltSvcPortValue": 8443
      },
      "listenAddress": ":8443"
    }
  ],
  "templatePageInfo": {
    "cacheControlValue": "public, max-age=60"
  },
  "mainPageInfo": {
    "title": "Go Server"
  },
  "pprofInfo": {
    "enabled": true
  },
  "staticFiles": [
    {
      "httpPath": "/command.js",
      "filePath": "static/command.js",
      "cacheControlValue": "public, max-age=60"
    },
    {
      "httpPath": "/proxy.js",
      "filePath": "static/proxy.js",
      "cacheControlValue": "public, max-age=60"
    },
    {
      "httpPath": "/style.css",
      "filePath": "static/style.css",
      "cacheControlValue": "public, max-age=60"
    },
    {
      "httpPath": "/favicon.ico",
      "filePath": "static/go-favicon.ico",
      "cacheControlValue": "public, max-age=60"
    }
  ],
  "commandConfiguration": {
    "maxConcurrentCommands": 1,
    "requestTimeoutMilliseconds": 2000,
    "semaphoreAcquireTimeoutMilliseconds": 200,
    "commands": [
      {
        "id": "ifconfig",
        "description": "ifconfig",
        "command": "ifconfig",
        "args": [
          "-a"
        ]
      },
      {
        "id": "netstat_an",
        "description": "netstat -an",
        "command": "netstat",
        "args": [
          "-a",
          "-n"
        ]
      },
      {
        "id": "sleep",
        "description": "sleep .5",
        "command": "sleep",
        "args": [
          ".5"
        ]
      }
    ]
  },
  "proxies": [
    {
      "id": "test_proxy",
      "description": "test proxy",
      "url": "http://www.google.com"
    },
    {
      "id": "test_proxy_2",
      "description": "test proxy 2",
      "url": "http://www.mprnews.org"
    }
  ]
}