{
  "listenAddress": ":8000",
  "tlsInfo": {
    "enabled": true,
    "certFile": "cert.pem",
    "keyFile": "key.pem"
  },
  "templatePageInfo": {
    "cacheControlValue": "public, max-age=60"
  },
  "mainPageInfo": {
    "title": "Aaron's Raspberry Pi"
  },
  "pprofInfo": {
    "enabled": true
  },
  "staticFiles": [
    {
      "httpPath": "/command.js",
      "filePath": "static/command.js",
      "cacheControlValue": "public, max-age=60"
    },
    {
      "httpPath": "/proxy.js",
      "filePath": "static/proxy.js",
      "cacheControlValue": "public, max-age=60"
    },
    {
      "httpPath": "/style.css",
      "filePath": "static/style.css",
      "cacheControlValue": "public, max-age=60"
    },
    {
      "httpPath": "/favicon.ico",
      "filePath": "static/raspberrypi-favicon.ico",
      "cacheControlValue": "public, max-age=60"
    }
  ],
  "commandTimeoutInfo": {
    "timeoutMilliseconds": 2000
  },
  "commands": [
    {
      "id": "arp",
      "command": "/usr/sbin/arp",
      "args": [
        "-an"
      ],
      "description": "arp"
    },
    {
      "id": "cpu_vulnerabilities",
      "command": "/bin/sh",
      "args": [
        "-c",
        "grep . /sys/devices/system/cpu/vulnerabilities/*"
      ],
      "description": "cpu vulnerabilities"
    },
    {
      "id": "df",
      "command": "df",
      "args": [
        "-h"
      ],
      "description": "df"
    },
    {
      "id": "dmesg",
      "command": "dmesg",
      "description": "dmesg"
    },
    {
      "id": "ifconfig",
      "command": "/sbin/ifconfig",
      "description": "ifconfig"
    },
    {
      "id": "iwconfig",
      "command": "/sbin/iwconfig",
      "description": "iwconfig"
    },
    {
      "id": "log_ls",
      "command": "ls",
      "args": [
        "-latrh",
        "/home/pi/pi-web/logs"
      ],
      "description": "log ls"
    },
    {
      "id": "log_tail",
      "command": "tail",
      "args": [
        "-n30",
        "/home/pi/pi-web/logs/output"
      ],
      "description": "log tail"
    },
    {
      "id": "lscpu",
      "command": "lscpu",
      "description": "lscpu"
    },
    {
      "id": "ncal",
      "command": "ncal",
      "args": [
        "-h",
        "-y"
      ],
      "description": "ncal"
    },
    {
      "id": "netstat_an",
      "command": "netstat",
      "args": [
        "-an"
      ],
      "description": "netstat -an"
    },
    {
      "id": "netstat_s",
      "command": "netstat",
      "args": [
        "-s"
      ],
      "description": "netstat -s"
    },
    {
      "id": "ntpq",
      "command": "ntpq",
      "args": [
        "-pn"
      ],
      "description": "ntpq"
    },
    {
      "id": "pitemp",
      "command": "/home/pi/bin/pitemp.sh",
      "description": "pitemp"
    },
    {
      "id": "tc_qdisc",
      "command": "/sbin/tc",
      "args": [
        "-s",
        "qdisc",
        "show"
      ],
      "description": "tc qdisc show"
    },
    {
      "id": "top",
      "command": "top",
      "args": [
        "-b",
        "-n1"
      ],
      "description": "top"
    },
    {
      "id": "unifi_log",
      "command": "sudo",
      "args": [
        "tail",
        "-n30",
        "/var/log/unifi/server.log"
      ],
      "description": "unifi log"
    },
    {
      "id": "uptime",
      "command": "uptime",
      "description": "uptime"
    },
    {
      "id": "vmstat",
      "command": "vmstat",
      "description": "vmstat"
    },
    {
      "id": "w",
      "command": "w",
      "description": "w"
    }
  ]
}
