{
  "logRequests": false,
  "listenInfoList": [
    {
      "listenAddress": "127.0.0.1:8080",
      "tlsInfo": {
        "enabled": false
      }
    }
  ],
  "templatePageInfo": {
    "cacheControlValue": "public, max-age=600"
  },
  "mainPageInfo": {
    "title": "aaronr.digital"
  },
  "pprofInfo": {
    "enabled": false
  },
  "staticFiles": [
    {
      "httpPath": "/command.js",
      "filePath": "static/command.js",
      "cacheControlValue": "public, max-age=600"
    },
    {
      "httpPath": "/proxy.js",
      "filePath": "static/proxy.js",
      "cacheControlValue": "public, max-age=600"
    },
    {
      "httpPath": "/style.css",
      "filePath": "static/style.css",
      "cacheControlValue": "public, max-age=600"
    },
    {
      "httpPath": "/favicon.ico",
      "filePath": "static/go-favicon.ico",
      "cacheControlValue": "public, max-age=600"
    }
  ],
  "staticDirectories": [
    {
      "httpPath": "/pfstat/hour/",
      "directoryPath": "/var/www/htdocs/aaronr.digital/pfstat/hour",
      "cacheControlValue": "public, max-age=150"
    },
    {
      "httpPath": "/pfstat/day/",
      "directoryPath": "/var/www/htdocs/aaronr.digital/pfstat/day",
      "cacheControlValue": "public, max-age=150"
    },
    {
      "httpPath": "/pfstat/week/",
      "directoryPath": "/var/www/htdocs/aaronr.digital/pfstat/week",
      "cacheControlValue": "public, max-age=150"
    }
  ],
  "commandConfiguration": {
    "maxConcurrentCommands": 10,
    "requestTimeoutMilliseconds": 2000,
    "semaphoreAcquireTimeoutMilliseconds": 200,
    "commands": [
      {
        "id": "ifconfig",
        "description": "ifconfig",
        "command": "ifconfig",
        "args": [
          "-a"
        ]
      },
      {
        "id": "netstat_an",
        "description": "netstat -an",
        "command": "netstat",
        "args": [
          "-a",
          "-n"
        ]
      },
      {
        "id": "netstat_binh",
        "description": "netstat -binh",
        "command": "netstat",
        "args": [
          "-binh"
        ]
      },
      {
        "id": "ntpctl_sa",
        "description": "ntpctl -sa",
        "command": "ntpctl",
        "args": [
          "-sa"
        ]
      },
      {
        "id": "relayctl_show_sessions",
        "description": "relayctl show sessions",
        "command": "relayctl",
        "args": [
          "show",
          "sessions"
        ]
      },
      {
        "id": "sysctl_hw",
        "description": "sysctl hw",
        "command": "sysctl",
        "args": [
          "hw"
        ]
      },
      {
        "id": "top",
        "description": "top",
        "command": "top",
        "args": [
          "-b1"
        ]
      },
      {
        "id": "uptime",
        "description": "uptime",
        "command": "uptime"
      },
      {
        "id": "vmstat",
        "description": "vmstat",
        "command": "vmstat"
      },
      {
        "id": "vmstat_i",
        "description": "vmstat -i",
        "command": "vmstat",
        "args": [
          "-i"
        ]
      },
      {
        "id": "w",
        "description": "w",
        "command": "w"
      }
    ]
  }
}
