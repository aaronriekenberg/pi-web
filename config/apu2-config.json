{
  "logRequests": false,
  "listenInfoList": [
    {
      "listenAddress": "192.168.1.1:8080",
      "tlsInfo": {
        "enabled": true,
        "certFile": "cert.pem",
        "keyFile": "key.pem"
      }
    },
    {
      "listenAddress": "[fd12:3456:789a:1::1]:8080",
      "tlsInfo": {
        "enabled": true,
        "certFile": "cert.pem",
        "keyFile": "key.pem"
      }
    }
  ],
  "templatePageInfo": {
    "cacheControlValue": "public, max-age=86400"
  },
  "mainPageInfo": {
    "title": "apu2.domain"
  },
  "pprofInfo": {
    "enabled": true
  },
  "staticFiles": [
    {
      "httpPath": "/command.js",
      "filePath": "static/command.js",
      "cacheControlValue": "public, max-age=86400"
    },
    {
      "httpPath": "/proxy.js",
      "filePath": "static/proxy.js",
      "cacheControlValue": "public, max-age=86400"
    },
    {
      "httpPath": "/style.css",
      "filePath": "static/style.css",
      "cacheControlValue": "public, max-age=86400"
    },
    {
      "httpPath": "/favicon.ico",
      "filePath": "static/go-favicon.ico",
      "cacheControlValue": "public, max-age=86400"
    }
  ],
  "staticDirectories": [
    {
      "httpPath": "/pfstat/hour/",
      "directoryPath": "/var/www/htdocs/apu2.domain/pfstat/hour",
      "cacheControlValue": "public, max-age=150",
      "includeInMainPage": true
    },
    {
      "httpPath": "/pfstat/day/",
      "directoryPath": "/var/www/htdocs/apu2.domain/pfstat/day",
      "cacheControlValue": "public, max-age=150",
      "includeInMainPage": true
    },
    {
      "httpPath": "/pfstat/week/",
      "directoryPath": "/var/www/htdocs/apu2.domain/pfstat/week",
      "cacheControlValue": "public, max-age=150",
      "includeInMainPage": true
    }
  ],
  "commandConfiguration": {
    "maxConcurrentCommands": 10,
    "requestTimeoutMilliseconds": 2000,
    "semaphoreAcquireTimeoutMilliseconds": 200,
    "commands": [
      {
        "id": "arp_an",
        "description": "arp -an",
        "command": "/usr/sbin/arp",
        "args": [
          "-an"
        ]
      },
      {
        "id": "ifconfig",
        "description": "ifconfig",
        "command": "ifconfig",
        "args": [
          "-a"
        ]
      },
      {
        "id": "ndp_an",
        "description": "ndp -an",
        "command": "/usr/sbin/ndp",
        "args": [
          "-an"
        ]
      },
      {
        "id": "netstat_an",
        "description": "netstat -an",
        "command": "netstat",
        "args": [
          "-a",
          "-n"
        ]
      },
      {
        "id": "netstat_binh",
        "description": "netstat -binh",
        "command": "netstat",
        "args": [
          "-binh"
        ]
      },
      {
        "id": "ntpctl_sa",
        "description": "ntpctl -sa",
        "command": "ntpctl",
        "args": [
          "-sa"
        ]
      },
      {
        "id": "sysctl_hw",
        "description": "sysctl hw",
        "command": "sysctl",
        "args": [
          "hw"
        ]
      },
      {
        "id": "top",
        "description": "top",
        "command": "top",
        "args": [
          "-b1"
        ]
      },
      {
        "id": "uptime",
        "description": "uptime",
        "command": "uptime"
      },
      {
        "id": "vmstat",
        "description": "vmstat",
        "command": "vmstat"
      },
      {
        "id": "vmstat_i",
        "description": "vmstat -i",
        "command": "vmstat",
        "args": [
          "-i"
        ]
      },
      {
        "id": "w",
        "description": "w",
        "command": "w"
      }
    ]
  }
}