{
  "listenAddress": "192.168.1.1:8000",
  "tlsInfo": {
    "enabled": true,
    "certFile": "cert.pem",
    "keyFile": "key.pem"
  },
  "templatePageInfo": {
    "cacheControlValue": "public, max-age=60"
  },
  "mainPageInfo": {
    "title": "Aaron's APU2"
  },
  "pprofInfo": {
    "enabled": true
  },
  "staticFiles": [
    {
      "httpPath": "/command.js",
      "filePath": "static/command.js",
      "cacheControlValue": "public, max-age=60"
    },
    {
      "httpPath": "/proxy.js",
      "filePath": "static/proxy.js",
      "cacheControlValue": "public, max-age=60"
    },
    {
      "httpPath": "/style.css",
      "filePath": "static/style.css",
      "cacheControlValue": "public, max-age=60"
    },
    {
      "httpPath": "/favicon.ico",
      "filePath": "static/go-favicon.ico",
      "cacheControlValue": "public, max-age=60"
    }
  ],
  "staticDirectories": [
    {
      "httpPath": "/pfstat/",
      "directoryPath": "/var/www/htdocs/pfstat"
    }
  ],
  "commands": [
    {
      "id": "arp",
      "command": "/usr/sbin/arp",
      "args": [
        "-an"
      ],
      "description": "arp"
    },
    {
      "id": "df",
      "command": "df",
      "args": [
        "-h"
      ],
      "description": "df"
    },
    {
      "id": "dhcp-lease-list",
      "command": "/home/aaron/dhcp-leases/show-leases.sh",
      "description": "dhcp lease list"
    },
    {
      "id": "dmesg",
      "command": "dmesg",
      "description": "dmesg"
    },
    {
      "id": "ifconfig",
      "command": "/sbin/ifconfig",
      "description": "ifconfig"
    },
    {
      "id": "log_ls",
      "command": "ls",
      "args": [
        "-latrh",
        "/home/aaron/pi-web/logs"
      ],
      "description": "log ls"
    },
    {
      "id": "log_tail",
      "command": "tail",
      "args": [
        "-n30",
        "/home/aaron/pi-web/logs/current"
      ],
      "description": "log tail"
    },
    {
      "id": "netstat_an",
      "command": "netstat",
      "args": [
        "-an"
      ],
      "description": "netstat -an"
    },
    {
      "id": "netstat_s",
      "command": "netstat",
      "args": [
        "-s"
      ],
      "description": "netstat -s"
    },
    {
      "id": "ntpctl",
      "command": "ntpctl",
      "args": [
        "-sa"
      ],
      "description": "ntpctl"
    },
    {
      "id": "sysctl_hw",
      "command": "sysctl",
      "args": [
        "hw"
      ],
      "description": "sysctl hw"
    },
    {
      "id": "top",
      "command": "top",
      "args": [
        "-b",
        "-n1"
      ],
      "description": "top"
    },
    {
      "id": "pfctl_si",
      "command": "pfctl",
      "args": [
        "-si",
        "-v"
      ],
      "description": "pfctl -si"
    },
    {
      "id": "pfctl_sq",
      "command": "pfctl",
      "args": [
        "-sq",
        "-v"
      ],
      "description": "pfctl -sq"
    },
    {
      "id": "uptime",
      "command": "uptime",
      "description": "uptime"
    },
    {
      "id": "vmstat",
      "command": "vmstat",
      "description": "vmstat"
    },
    {
      "id": "vmstat_i",
      "command": "vmstat",
      "args": [
        "-i"
      ],
      "description": "vmstat -i"
    },
    {
      "id": "w",
      "command": "w",
      "description": "w"
    }
  ]
}
