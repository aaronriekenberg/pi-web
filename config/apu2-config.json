{
  "logRequests": false,
  "listenAddress": "192.168.1.1:8000",
  "tlsInfo": {
    "enabled": true,
    "certFile": "cert.pem",
    "keyFile": "key.pem"
  },
  "templatePageInfo": {
    "cacheControlValue": "public, max-age=60"
  },
  "mainPageInfo": {
    "title": "Aaron's APU2"
  },
  "pprofInfo": {
    "enabled": true
  },
  "staticFiles": [
    {
      "httpPath": "/command.js",
      "filePath": "static/command.js",
      "cacheControlValue": "public, max-age=60"
    },
    {
      "httpPath": "/proxy.js",
      "filePath": "static/proxy.js",
      "cacheControlValue": "public, max-age=60"
    },
    {
      "httpPath": "/style.css",
      "filePath": "static/style.css",
      "cacheControlValue": "public, max-age=60"
    },
    {
      "httpPath": "/favicon.ico",
      "filePath": "static/go-favicon.ico",
      "cacheControlValue": "public, max-age=60"
    }
  ],
  "staticDirectories": [
    {
      "httpPath": "/vnstat/",
      "directoryPath": "/home/aaron/vnstat"
    }
  ],
  "commandTimeoutInfo": {
    "timeoutMilliseconds": 2000
  },
  "commands": [
    {
      "id": "arp",
      "command": "/usr/sbin/arp",
      "args": [
        "-an"
      ],
      "description": "arp"
    },
    {
      "id": "cpu_vulnerabilities",
      "command": "/bin/bash",
      "args": [
        "-c",
        "grep . /sys/devices/system/cpu/vulnerabilities/*"
      ],
      "description": "cpu_vulnerabilities"
    },
    {
      "id": "df",
      "command": "df",
      "args": [
        "-h"
      ],
      "description": "df"
    },
    {
      "id": "go_dhcp_leases",
      "command": "/home/aaron/go-dhcp-leases/run.sh",
      "description": "go dhcp leases"
    },
    {
      "id": "go_doh_proxy_logs",
      "command": "journalctl",
      "args": [
        "--user-unit",
        "go-doh-proxy.service",
        "-n30",
        "--no-pager",
        "--output=cat"
      ],
      "description": "go-doh-proxy logs"
    },
    {
      "id": "ifconfig",
      "command": "/sbin/ifconfig",
      "description": "ifconfig"
    },
    {
      "id": "lscpu",
      "command": "lscpu",
      "description": "lscpu"
    },
    {
      "id": "netstat_an",
      "command": "netstat",
      "args": [
        "-an"
      ],
      "description": "netstat -an"
    },
    {
      "id": "netstat_s",
      "command": "netstat",
      "args": [
        "-s"
      ],
      "description": "netstat -s"
    },
    {
      "id": "node_dns_logs",
      "command": "journalctl",
      "args": [
        "--user-unit",
        "node-dns.service",
        "-n30",
        "--no-pager",
        "--output=cat"
      ],
      "description": "node-dns logs"
    },
    {
      "id": "ntpq",
      "command": "ntpq",
      "args": [
        "-pn"
      ],
      "description": "ntpq"
    },
    {
      "id": "pi_web_logs",
      "command": "journalctl",
      "args": [
        "--user-unit",
        "pi-web.service",
        "-n30",
        "--no-pager"
      ],
      "description": "pi-web logs"
    },
    {
      "id": "rust_doh_proxy_logs",
      "command": "journalctl",
      "args": [
        "--user-unit",
        "rust-doh-proxy.service",
        "-n30",
        "--no-pager"
      ],
      "description": "rust-doh-proxy logs"
    },
    {
      "id": "sensors",
      "command": "sensors",
      "description": "sensors"
    },
    {
      "id": "tc_qdisc",
      "command": "/sbin/tc",
      "args": [
        "-s",
        "qdisc",
        "show"
      ],
      "description": "tc qdisc show"
    },
    {
      "id": "top",
      "command": "top",
      "args": [
        "-b",
        "-n1"
      ],
      "description": "top"
    },
    {
      "id": "uptime",
      "command": "uptime",
      "description": "uptime"
    },
    {
      "id": "vmstat",
      "command": "vmstat",
      "description": "vmstat"
    },
    {
      "id": "w",
      "command": "w",
      "description": "w"
    }
  ]
}
