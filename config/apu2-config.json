{
  "listenAddress": "192.168.1.1:8000",
  "tlsInfo": {
    "enabled": true,
    "certFile": "cert.pem",
    "keyFile": "key.pem"
  },
  "templatePageInfo": {
    "cacheControlValue": "public, max-age=60"
  },
  "mainPageInfo": {
    "title": "Aaron's APU2"
  },
  "pprofInfo": {
    "enabled": true
  },
  "staticFiles": [
    {
      "httpPath": "/command.js",
      "filePath": "static/command.js",
      "cacheControlValue": "public, max-age=60"
    },
    {
      "httpPath": "/proxy.js",
      "filePath": "static/proxy.js",
      "cacheControlValue": "public, max-age=60"
    },
    {
      "httpPath": "/style.css",
      "filePath": "static/style.css",
      "cacheControlValue": "public, max-age=60"
    },
    {
      "httpPath": "/favicon.ico",
      "filePath": "static/go-favicon.ico",
      "cacheControlValue": "public, max-age=60"
    }
  ],
  "staticDirectories": [
    {
      "httpPath": "/vnstat/",
      "directoryPath": "/home/aaron/vnstat"
    }
  ],
  "commandTimeoutInfo": {
    "timeoutMilliseconds": 2000
  },
  "commands": [
    {
      "id": "arp",
      "command": "/usr/sbin/arp",
      "args": [
        "-an"
      ],
      "description": "arp"
    },
    {
      "id": "cpu_vulnerabilities",
      "command": "/bin/bash",
      "args": [
        "-c",
        "grep . /sys/devices/system/cpu/vulnerabilities/*"
      ],
      "description": "cpu_vulnerabilities"
    },
    {
      "id": "df",
      "command": "df",
      "args": [
        "-h"
      ],
      "description": "df"
    },
    {
      "id": "go_dns_log",
      "command": "tail",
      "args": [
        "-n30",
        "/home/aaron/go-dns/logs/output"
      ],
      "description": "go dns log"
    },
    {
      "id": "ifconfig",
      "command": "/sbin/ifconfig",
      "description": "ifconfig"
    },
    {
      "id": "log_ls",
      "command": "ls",
      "args": [
        "-latrh",
        "/home/aaron/pi-web/logs"
      ],
      "description": "log ls"
    },
    {
      "id": "log_tail",
      "command": "tail",
      "args": [
        "-n30",
        "/home/aaron/pi-web/logs/output"
      ],
      "description": "log tail"
    },
    {
      "id": "lscpu",
      "command": "lscpu",
      "description": "lscpu"
    },
    {
      "id": "netstat_an",
      "command": "netstat",
      "args": [
        "-an"
      ],
      "description": "netstat -an"
    },
    {
      "id": "netstat_s",
      "command": "netstat",
      "args": [
        "-s"
      ],
      "description": "netstat -s"
    },
    {
      "id": "node_dns_log",
      "command": "tail",
      "args": [
        "-n30",
        "/home/aaron/node-dns/logs/output"
      ],
      "description": "node dns log"
    },
    {
      "id": "ntpq",
      "command": "ntpq",
      "args": [
        "-p"
      ],
      "description": "ntpq"
    },
    {
      "id": "sensors",
      "command": "sensors",
      "description": "sensors"
    },
    {
      "id": "tc_qdisc",
      "command": "/sbin/tc",
      "args": [
        "-s",
        "qdisc",
        "show"
      ],
      "description": "tc qdisc show"
    },
    {
      "id": "top",
      "command": "top",
      "args": [
        "-b",
        "-n1"
      ],
      "description": "top"
    },
    {
      "id": "uptime",
      "command": "uptime",
      "description": "uptime"
    },
    {
      "id": "vmstat",
      "command": "vmstat",
      "description": "vmstat"
    },
    {
      "id": "w",
      "command": "w",
      "description": "w"
    }
  ]
}
