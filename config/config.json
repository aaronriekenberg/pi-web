{
  "listenAddress": ":8000",
  "tlsInfo": {
    "enabled": true,
    "certFile": "cert.pem",
    "keyFile": "key.pem"
  },
  "mainPageInfo": {
    "title": "Go Server",
    "cacheControlValue": "public, max-age=60"
  },
  "pprofInfo": {
    "enabled": true
  },
  "staticFiles": [
    {
      "httpPath": "/command.js",
      "filePath": "static/command.js",
      "cacheControlValue": "public, max-age=60"
    },
    {
      "httpPath": "/proxy.js",
      "filePath": "static/proxy.js",
      "cacheControlValue": "public, max-age=60"
    },
    {
      "httpPath": "/style.css",
      "filePath": "static/style.css",
      "cacheControlValue": "public, max-age=60"
    }
  ],
  "commands": [
    {
      "id": "ifconfig",
      "description": "ifconfig",
      "command": "ifconfig",
      "args": [
        "-a"
      ]
    },
    {
      "id": "netstat_an",
      "description": "netstat -an",
      "command": "netstat",
      "args": [
        "-a",
        "-n"
      ]
    }
  ],
  "proxies": [
    {
      "id": "test_proxy",
      "description": "test proxy",
      "url": "http://www.google.com"
    }
  ]
}
